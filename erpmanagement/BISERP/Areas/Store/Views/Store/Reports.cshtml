    function loadUserReportCategory(menuId) {
        $.ajax({
            type: "GET",
            url: "Store/Store/GetUserReports",
            data: { MenuId: menuId },
            datatype: "Json",
            success: function (response) {
                let data = response.model;
                if (data.length > 0) {
                    $("#ddlReport").append('<option value="0">Select</option>');
                    $.each(data, function (index, value) {
                        $("#ddlReport").append('<option value="' + value.ReportID + '">' + value.ReportName + '</option>');
                    });
                }
                else {
                    ShowAlert("warning", "No Data Found.");
                }
            }
        });
    }

    function GetStoreFinancialYearWise(financialYear) {
        $("#ddlStoreLabel").autocomplete({
            source: [],
            minLength: 0,
            select: handleStoreSelection,
            change: handleStoreSelection,
        });

        $.ajax({
            type: "GET",
            url: "/Master/GetStoreFinancialYearWise",
            data: { financialYear },
            datatype: "Json",
            async: true,
            success: function (data) {
                if (data && data.length > 0) {
                    const suggestions = data.map(function (store) {
                        return {
                            label: store.Name,
                            value: store.ID
                        };
                    });
                    $("#ddlStoreLabel").autocomplete("option", "source", suggestions);
                } else {
                    $("#ddlStoreLabel").autocomplete("option", "source", [{ label: "--No Stores Found--", value: "" }]);
                }
            },
            error: function () {
                $("#ddlStoreLabel").autocomplete("option", "source", [{ label: "--Error loading stores--", value: "" }]);
            }
        });
    }

    // This function will handle the store selection logic
    function handleStoreSelection(event, ui) {
        event.preventDefault();
        const $storeLabel = $(event.target);
        const $storeID = $("#ddlStore");

        if (ui.item && ui.item.value) {
            if (ui.item.label === "--No Stores Found--" || ui.item.label === "--Error loading stores--") {
                $storeLabel.val('');
                $storeID.val('').trigger('change');
            } else {
                $storeLabel.val(ui.item.label.trim())
                $storeID.val(ui.item.value).trigger('change');
            }
        } else {
            $storeLabel.val('');
            $storeID.val('').trigger('change');
        }
    }
    // Trigger the autocomplete dropdown when the user focuses on the input field
    $("#ddlStoreLabel").on("focus", function () {
        $(this).autocomplete("search", "");
    });



