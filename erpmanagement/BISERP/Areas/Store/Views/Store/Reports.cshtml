    function loadGrnStore() {
        $("#ddlStoreLabel").autocomplete({
            source: [],
            minLength: 0,
            select: handleStoreSelection,
            change: handleStoreSelection,
        });

        $.ajax({
            type: "GET",
            url: "/Master/GetStoreFinancialYearWise",
            datatype: "Json",
            async: true,
            success: function (data) {
                if (data && data.length > 0) {
                    const suggestions = data.map(function (store) {
                        return {
                            label: store.Name,
                            value: store.ID
                        };
                    });
                    $("#ddlStoreLabel").autocomplete("option", "source", suggestions);
                } else {
                    $("#ddlStoreLabel").autocomplete("option", "source", [{ label: "--No Stores Found--", value: "" }]);
                }
            },
            error: function () {
                $("#ddlStoreLabel").autocomplete("option", "source", [{ label: "--Error loading stores--", value: "" }]);
            }
        });
    }

