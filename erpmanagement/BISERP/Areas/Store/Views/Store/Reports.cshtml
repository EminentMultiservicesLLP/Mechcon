<script>
    $(document).ready(function () {
        loadUserReportCategory(76);
        ClearForm();
    });

    function loadFinancialYear() {
        const $financialYear = $('#financialYear');

        $.ajax({
            type: "GET",
            url: "/ResourceTarget/GetFinancialYear",
            dataType: "json",
            success: function (data) {
                $financialYear.empty();

                if (data.error) {
                    $financialYear.append('<option value="">--Error loading financial years--</option>');
                    console.error("Error loading financial years:", data.error);
                    return;
                }

                if (data.length) {
                    data.forEach(({ FinancialYear }) => {
                        $financialYear.append(`<option value="${FinancialYear}">${FinancialYear}</option>`);
                    });

                    const currentFinancialYear = getCurrentFinancialYear();
                    $financialYear.val(currentFinancialYear);

                    GetStoreFinancialYearWise(currentFinancialYear);

                    const { startDate, endDate } = getFinancialYearDates(currentFinancialYear);
                    $("#FromDate").datepicker("setDate", startDate);
                    $("#ToDate").datepicker("setDate", endDate);
                } else {
                    $financialYear.append('<option value="">--No financial years available--</option>');
                }
            },
            error: function (xhr, textStatus, errorThrown) {
                $financialYear.empty();
                $financialYear.append('<option value="">--Error loading financial years--</option>');
                console.error("Error loading financial years:", errorThrown);
            }
        });
    }

    loadFinancialYear();

    $("#financialYear").on("change", function () {
        const financialYear = $(this).val();
        GetStoreFinancialYearWise(financialYear);
        const { startDate, endDate } = getFinancialYearDates(financialYear);
        $("#FromDate").datepicker("setDate", startDate);
        $("#ToDate").datepicker("setDate", endDate);
    });


    $("#ddlStore").on('change', function () {
        const storeId = $("#ddlStore").val(); // Store the value once

        if (storeId) { // Checks for null, undefined, or empty string
            if ($("#tab7").is(':visible')) { // Use `is` for readability
                $("#PackingListNo").val("");
            } else {
                $.ajax({
                    type: "GET",
                    url: "/Master/GetItemTypeByStoreId",
                    data: { storeId },
                    dataType: "json", // Corrected casing for `dataType`
                    success: function (data) {
                        const $ddlItemtype = $("#ddlItemtype");
                        $ddlItemtype.empty(); // Clear the dropdown
                        $ddlItemtype.append('<option value="0">All</option>'); // Add default option

                        // Append each item from the response
                        $.each(data, function (index, value) {
                            $ddlItemtype.append(`<option value="${value.ID}">${value.Name}</option>`);
                        });
                    },
                    error: function (xhr, status, error) {
                        console.error("Error fetching item types:", error);
                        alert("An error occurred while loading item types. Please try again.");
                    }
                });
            }
        }
    });


    //$("#FromDate").on('change', function () {
    //    $("#PackingListNo").val() = '';
    //})

    //$("#ToDate").on('change', function () {
