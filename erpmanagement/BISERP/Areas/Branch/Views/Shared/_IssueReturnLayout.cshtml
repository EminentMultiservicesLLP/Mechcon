
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Branch Module</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/themes/base/css")
    @Styles.Render("~/Content/external")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jqueryMore")
    @Scripts.Render("~/bundles/jquery/external")
    @Scripts.Render("~/bundles/jqueryval")
</head>
<body>
    <header>
        <div class="row">
            <div class="col-sm-2"></div>
            <div class="col-sm-6">
                <div class="float-left">
                    <p class="site-title">
                        <a href="../../../Home/Index">
                            <img src="@Url.Content("~/Content/images/logo2.png")" />
                            
                        </a>
                    </p>
                </div>
            </div>
            <div class="col-sm-3">
                <div class="float-right">
                    <ul class="nav navbar-nav">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <strong>@Html.Encode(User.Identity.Name)</strong>&nbsp;&nbsp;&nbsp;&nbsp;
                                <span class="glyphicon glyphicon-user pull-right"></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li><a href="~/Account/LogOff">Sign Out &nbsp;&nbsp;<span class="glyphicon glyphicon-log-out"></span></a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>        
        <div class="content-wrapper">
            <div id="body">
                @RenderBody()
            </div>
        </div>
    </header>
    @RenderSection("scripts", required: false)
</body>

</html>
